{
    "name": "Mopidy Media Server",
    "version": "0.2.0",
    "slug": "mopidy-media-server",
    "description": "Mopidy music server with additional utilities",
    "startup": "application",
    "boot": "auto",
    "arch": ["armhf"],
    "audio": true,
    "options": {},
    "schema": {},
